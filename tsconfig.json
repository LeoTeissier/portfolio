{
  "extends":"./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target":"esnext",
    "module":"esnext",
    "moduleResolution":"node",
    "strict":true,
    "jsx":"preserve",
    "importHelpers":true,
    "esModuleInterop":true,
    "allowSyntheticDefaultImports":true,
    "sourceMap":true,
    "baseUrl":".",
    "paths":{"@/*":["src/*"]},
    "types":["vite/client"],
    "allowImportingTsExtensions":true,
    "resolveJsonModule":true
  },
  "include":["src/**/*.ts","src/**/*.d.ts","src/**/*.vue", "app.config.ts"],
  "exclude":["node_modules"]
}